          "type": "text",
          "settings": {
            "text": "<p><strong>This Privacy Policy describes how your personal information is collected, used, and shared when you visit or make a purchase from theresortco.com (the “Site”).</strong></p>",
            "text_font_size": 2
          }
        },
        "divider_LqYypD": {
          "type": "divider",
          "settings": {
            "thickness": 1,
            "space_above": 20,
            "space_below": 40,
            "space_above_mobile": 10,
